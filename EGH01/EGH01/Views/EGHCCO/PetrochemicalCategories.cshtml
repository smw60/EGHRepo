@using EGH01.Core;
@using EGH01.Models.EGH;
@using EGH01DB.Primitives;
@using EGH01DB.Types;
@model EGH01DB.CCOContext


@{
    ViewBag.Title = "PetrochemicalCategories";
    Layout = "~/Views/Shared/EGHLayout.cshtml";

    Menu head = new Menu(
                         new Menu.MenuItem("Создать", "PetrochemicalCategories.Create", true),
                         new Menu.MenuItem("Excel-формат", "PetrochemicalCategories.Excel", true)
                       );

    List<PetrochemicalCategories> list = new List<PetrochemicalCategories>();
    if (@Model != null) { Helper.GetListPetrochemicalCategories(@Model, ref list); };


}


@Html.Partial("_TypeHeadMenu", new TypeViewMenu { Controller = "EGHCCO", Action = "PetrochemicalCategories", Menu = head })
@Html.Partial("_PetrochemicalCategoriesList", list)

