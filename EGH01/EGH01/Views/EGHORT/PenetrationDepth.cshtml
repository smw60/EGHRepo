@using EGH01.Core;
@using EGH01.Models.EGH;
@using EGH01DB.Primitives;
@using EGH01DB.Types;
@using EGH01DB.Objects;
@model EGH01DB.ORTContext
@{


    ViewBag.Title = "PenetrationDepth";
    Layout = "~/Views/Shared/EGHLayout.cshtml";
    Menu head = new Menu(
                          new Menu.MenuItem("Создать", "PenetrationDepth.Create", true),
                          new Menu.MenuItem("Excel-формат", "PenetrationDepth.Excel", true)
                        );

    List<PenetrationDepth> list = new List<PenetrationDepth>();
    if (@Model != null) { Helper.GetListPenetrationDepth(@Model, ref list); };

}


@Html.Partial("_TypeHeadMenu", new TypeViewMenu { Controller = "EGHORT", Action = "PenetrationDepth", Menu = head })
@Html.Partial("_PenetrationDepthList", list)







