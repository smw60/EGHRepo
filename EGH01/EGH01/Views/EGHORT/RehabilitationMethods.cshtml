@using EGH01.Core;
@using EGH01.Models.EGH;
@using EGH01DB.Primitives;
@using EGH01DB.Types;
@using EGH01DB.Objects;
@model EGH01DB.ORTContext
@{


    ViewBag.Title = "RehabilitationMethods";
    Layout = "~/Views/Shared/EGHLayout.cshtml";
    Menu head = new Menu(
                          new Menu.MenuItem("Создать", "RehabilitationMethods.Create", true),
                          new Menu.MenuItem("Excel-формат", "RehabilitationMethods.Excel", true)
                        );

    List<RehabilitationMethod> list = new List<RehabilitationMethod>();
    if (@Model != null) { Helper.GetListRehabilitationMethod(@Model, ref list); };

}

@Html.Partial("_TypeHeadMenu", new TypeViewMenu { Controller = "EGHORT", Action = "RehabilitationMethods", Menu = head })
@Html.Partial("_RehabilitationMethods", list)

















