@using EGH01.Core;
@using EGH01.Models.EGH;
@using EGH01DB.Primitives;
@using EGH01DB.Types;
@model EGH01DB.ORTContext


@{
    ViewBag.Title = "WaterProtectionArea";
    Layout = "~/Views/Shared/EGHLayout.cshtml";

    Menu head = new Menu(
                         new Menu.MenuItem("Создать", "WaterProtectionArea.Create", true),
                         new Menu.MenuItem("Excel-формат", "WaterProtectionArea.Excel", true)
                       );

    List<WaterProtectionArea> list = new List<WaterProtectionArea>();
    if (@Model != null) { Helper.GetListWaterProtectionArea(@Model, ref list); };


}


@Html.Partial("_TypeHeadMenu", new TypeViewMenu { Controller = "EGHORT", Action = "WaterProtectionArea", Menu = head })
@Html.Partial("_WaterProtectionAreaList", list)

