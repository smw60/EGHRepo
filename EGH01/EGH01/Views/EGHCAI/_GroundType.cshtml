@using EGH01.Core;
@using EGH01.Models.EGH;
@using EGH01DB.Primitives;
@using EGH01DB.Types;
@model List<GroundType>
@{
    Menu menu = new Menu(
                            new Menu.MenuItem("Удалить", "RiskObject.Delete", true),
                            new Menu.MenuItem("Изменить", "RiskObject.Update", true)
                           );

}
@foreach (EGH01DB.Types.GroundType t in @Model)
{

    using (Html.BeginForm("RiskObject", "EGHCAI"))
    {
      <div class="box">
            <div class="boxdata">

                <p><label class="boxdata-label-30">Тип</label> @t.type_code</p>
                <p><label class="boxdata-label-30">Наименование</label> @t.name</p> 
                <p><label class="boxdata-label-30">Пористость</label> @t.porosity</p>  
                <p><label class="boxdata-label-30">Коэфф. задержки миграции нефтепродуктов</label> @t.holdmigration</p>
                <p><label class="boxdata-label-30">Коэфф. фильтрации воды</label> @t.waterfilter</p>
                <p><label class="boxdata-label-30">Коэфф. диффузии</label> @t.diffusion</p>             
                <p><label class="boxdata-label-30">Коэфф. распределения</label> @t.distribution</p>
                <p><label class="boxdata-label-30">Коэфф. сорбции</label> @t.sorption</p> 
             </div>
                 </div>

    }
}

 